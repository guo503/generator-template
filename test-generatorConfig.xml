<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包-->
	<classPathEntry location="D:\project\generator-template\mysql-connector-java-5.1.46.jar"/>

	   <!--<classPathEntry location="mysql-connector-java-5.1.46.jar"/>-->
	
    <context id="Mysql" targetRuntime="MyBatis3">
	    
		<!-- 注释用-->
		<property name="author" value="guos"/>
		
		<!-- 要继承的page类所在包-->
		<property name="page" value="com.tsyj.page.Page"/>
		
        <property name="javaFileEncoding" value="UTF-8"/>
		
		<!-- po要继承的page全路径-->
		<!-- <property name="page" value="com.panda.sdk.Page"/>-->
		
		 <!-- 配置pojo的序列化 -->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		  <!-- 去掉get,set方法，添加Lombok注解-->
		 <plugin type="org.mybatis.generator.plugins.LombokPlugin" >
            <property name="hasLombok" value="true"/>
        </plugin>
		
		
		
		    <!--基础服务项目路径前缀-->
        <prop name="core_project_prefix" value="D:\project\ac-core\course" enable="true"/>
		
			    <!--基础服务包路径前缀-->
        <prop name="core_package_prefix" value="com.ac.core.course" enable="true"/>
		
	    <!--api项目路径前缀-->
        <prop name="api_project_prefix" value="D:\project\ac-parent\ac-crm" enable="true"/>
		
	    <!--api包路径前缀-->
        <prop name="api_package_prefix" value="com.ac.crm" enable="true"/>

		
		
		 <!-- 
			    BaseMethodPlugin插件(xml,service,business,controller,基础方法配置)
				1.deleteByCondition        				 生成物理删除方法
				2.enableInsertSelective	        				 生成save方法
				3.enableSelectByPrimaryKey	             		 生成get方法
				4.enableUpdateByPrimaryKeySelective	         	 生成update方法
				5.countByCondition	                  			 生成listByIds方法
				6.listByCondition	         					 生成count方法
			    7.listByCondition	         					 生成list方法
		-->
		<plugin type="org.mybatis.generator.plugins.BaseMethodPlugin">		
		    <prop name="deleteByCondition" value="" enable="true"/>
			<prop name="insertSelective" value="save" enable="true"/>
			<prop name="selectByPrimaryKey" value="get" enable="true"/>
			<prop name="updateByPrimaryKeySelective" value="update" enable="true"/>
			<prop name="listByIds" value="listByIds" enable="true"/>
		    <prop name="count" value="count" enable="true"/>
		    <prop name="list" value="list" enable="true"/>
			<prop name="countByCondition" value="countByCondition" enable="true"/>
		    <prop name="listByCondition" value="listByCondition" enable="true"/>
		</plugin>
		
		
		
		  <!-- 
			   ExtendModelPlugin插件(生成vo,cond,mapper配置)
				1.generatorVo：          是否重新生成vo
				2.voProject：	         vo所在模块
				3.voPack：	             vo类所在包
				4.voSuffix：	         vo类后缀
				5.condSuffix:            cond类后缀
				6.generatorPo:           是否重新生成po：     不写或者空或者false,不生成
				7.generatorMapper:       是否重新生成Mapper:  不写或者空或者false,不生成
				8.generatorXml:          是否重新生成Xml：    不写或者空或者false,不生成
				9.mapper:                mapper继承的基类
			    9.softDeleteMapper:      mapper继承的逻辑删除基类
				10.table:                po类表注释
		        11.id:                   主键注解
				12.version:              乐观锁注解
				13.condition:            Condition查询类
				14.limitCondition:       limitCondition查询类
				15.logicDelete           逻辑删除注解
				
		-->
		<plugin type="org.mybatis.generator.plugins.ExtendModelPlugin">
						
			<prop name="voProject" value="D:\project\test\springboot-base\src\main\java" enable="true"/>
	
			<prop name="voPack" value="com.tsyj.vo" enable=""/>

			<prop name="voSuffix" value="VO" enable=""/>
			
			
			<prop name="aoProject" value="D:\project\test\springboot-base\src\main\java" enable="true"/>
	
			<prop name="aoPack" value="com.tsyj.ao" enable=""/>

			<prop name="aoSuffix" value="AO" enable=""/>
			
			
			<prop name="generatorPo" value="false" enable=""/>
			
			<prop name="generatorMapper" value="false" enable=""/>
			
			<prop name="generatorXml" value="false" enable=""/>
			
			
			<prop name="mapper" value="mybatis.base.mapper.Mapper" enable=""/>
					
			<prop name="softDeleteMapper" value="mybatis.base.mapper.SoftDeleteMapper" enable=""/>
							
		    <prop name="table" value="mybatis.core.annotation.Table" enable=""/>
			
			<prop name="id" value="mybatis.core.annotation.Id" enable=""/>
			
			<prop name="version" value="mybatis.core.annotation.Version" enable=""/>
			
		    <prop name="logicDelete" value="mybatis.core.annotation.LogicDelete" enable=""/>
			
			<prop name="condition" value="mybatis.core.entity.Condition" enable=""/>
			
			<prop name="limitCondition" value="mybatis.core.entity.LimitCondition" enable=""/>
			
		</plugin>
		
		
		
		
	    <plugin type="org.mybatis.generator.plugins.ManagePlugin">
		
		    <prop name="manageProject" value="D:\project\test\springboot-base\src\main\java" enable="true"/>
			
			<prop name="manageImplProject" value="D:\project\test\springboot-base\src\main\java" enable="true"/>
	
			<prop name="managePack" value="com.tsyj.service.business" enable="true"/>
			
			<prop name="manageImplPack" value="com.tsyj.service.business.impl" enable="true"/>
		     
			<prop name="manageSuffix" value="Business" enable="true"/>


			<prop name="creator" value="creator" enable=""/>			
			<prop name="updater" value="updater" enable=""/>		
			<prop name="create_time" value="create_time" enable=""/>			
			<prop name="update_time" value="update_time" enable=""/>		
			<prop name="userNameMethod" value="com.tsyj.consts.UserConst.DEFAULT_NAME" enable=""/>	
			<prop name="dateMethod" value="" enable=""/>	
			<prop name="exceptionPack" value="" enable=""/>	
	        <prop name="enableLogger" value="false" enable=""/>
				
			
		    <prop name="mapByIds" value="mapByIds" enable="true"/>
		    <prop name="map" value="map" enable="true"/>
            <prop name="listId" value="listId" enable="true"/>
			<prop name="saveAndGet" value="saveAndGet" enable="true"/>
			
	
		</plugin>
		
		
		 <!-- 
			servcie生成插件
				1.serviceProject：       接口所在模块
				2.servicePack：	         接口所在包
				3.remote：	             remote注解所在包
				4.applicationName：	     Application类所在包
				5.serviceImplProject：	 实现类所在模块
				6.serviceImplPack：	     实现类所在包
				16.mapByIds:	         生成mapByIds方法
				17.map:	                 生成map方法
				18.listId:	             生成listId方法
				19.saveAndReturnId:	     生成插入返回id方法
		-->
		<plugin type="org.mybatis.generator.plugins.ServicePlugin">
		
			<prop name="serviceProject" value="D:\project\test\springboot-base\src\main\java" enable=""/>
			
			<prop name="serviceImplProject" value="D:\project\test\springboot-base\src\main\java" enable=""/>
	
			<prop name="servicePack" value="com.tsyj.service" enable=""/>
			
			<prop name="serviceSuffix" value="Service" enable="true"/>
			
			<prop name="serviceImplPack" value="com.tsyj.service.impl" enable=""/>
			
		    <prop name="remote" value="" enable=""/>
		  
		    <prop name="applicationName" value="" enable=""/>

	        <prop name="enableLogger" value="false" enable=""/>
			
			
			<prop name="mapByIds" value="mapByIds" enable="true"/>
		    <prop name="map" value="map" enable="true"/>
            <prop name="listId" value="listId" enable="true"/>
			<prop name="saveAndGet" value="saveAndGet" enable="true"/>
			
	
		</plugin>
		
		
		
		<!-- 
			businessPlugin生成插件
				1.remoteResource：	            remoteResource类所在包
				2.businessProject：	            business类所在模块
				3.businessPack：	            business类所在包
				5.businessImplProject：	        business实现类所在模块
				6.businessImplPack：	        business实现类所在包
				7.businessSuffix                business类后缀
				8.doBatchMethod                 是否生成doBatch方法
				9.enableLogger                  是否生成logger日志
				10.exceptionPack:	            自定义异常类全路径
		-->
		<plugin type="org.mybatis.generator.plugins.BusinessPlugin">	
			
		    <prop name="remoteResource" value="" enable=""/>
			
			<prop name="modelConvertUtils" value="com.tsyj.utils.ModelConvertUtils" enable=""/>
					
			<prop name="businessProject" value="D:\project\test\springboot-base\src\main\java" enable=""/>
			
			<prop name="businessPack" value="com.tsyj.business" enable=""/>
			
			<prop name="businessImplProject" value="D:\project\test\springboot-base\src\main\java" enable=""/>
			
			<prop name="businessImplPack" value="com.tsyj.business.impl" enable=""/>
			
			<prop name="businessSuffix" value="Business" enable=""/>
			
			<prop name="enableLogger" value="false" enable=""/>
			
			<prop name="exceptionPack" value="" enable=""/>	
			
			<prop name="doBatchMethod" value="doBatch" enable="true"/>
			
		</plugin>
		
		
		
		
		
		<!-- 
			controller生成插件
				1.controllerProject：           controller所在模块
				2.controllerPack：	            controller所在包
				3.controllerSuffix：	        controller类后缀
				4.responseMethod	            Result类方法
				5.baseController:	            要继承的基础controller
				6.enableLogger                  是否生成logger日志
		--> 
		<plugin type="org.mybatis.generator.plugins.ControllerPlugin">
		
			<prop name="controllerProject" value="D:\project\test\springboot-base\src\main\java" enable=""/>
	
			<prop name="controllerPack" value="com.tsyj.controller" enable=""/>

			<prop name="controllerSuffix" value="Controller" enable=""/>
			
			<prop name="responseMethod" value="com.tsyj.response.Result:success($)|fail($)|success($,$)" enable=""/>
			
			<prop name="baseController" value="" enable=""/>
			
			<prop name="enableLogger" value="false" enable=""/>
			
		</plugin>
		
		
		
        <commentGenerator type="org.mybatis.generator.MyCommentGenerator">
            <property name="suppressDate" value="true"/>
			
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false"/>
			
			<!-- 自定义注释 -->
			<property name="addRemarkComments" value="true" />
					
        </commentGenerator>
        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://127.0.0.1:3306/tsyj?characterEncoding=utf8&amp;useInformationSchema=true&amp;useSSL=false" userId="root"
                        password="root">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- 生成模型的包名和位置-->
        <javaModelGenerator targetPackage="com.tsyj.model" targetProject="D:\project\test\springboot-base\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mapper" targetProject="D:\project\test\springboot-base\src\main\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.tsyj.mapper" targetProject="D:\project\test\springboot-base\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!-- 要生成的表 tableName是数据库中的表名或视图名 domainObjectName是实体类名-->
		  <!--  
		    1.enableBusiness:      是否生成business
			2.enableController:    是否生成controller
			3.versions             乐观锁列名
			4.enableVersions       是否启用乐观锁,只有versions配置才行
			5.逻辑删除列
		   -->
			<table tableName="country_code" domainObjectName="" enableManage="true" enableService="true" enableBusiness="true" enableController="true" versionCol="versions" enableVersions="true" delCol="is_del"/>
    </context>
</generatorConfiguration>